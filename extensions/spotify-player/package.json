{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spotify-player",
  "title": "Spotify Player Beta",
  "description": "Beta Version of the Spotify Player",
  "icon": "icon.png",
  "author": "mattisssa",
  "owner": "raycast",
  "license": "MIT",
  "commands": [
    {
      "name": "searchTracks",
      "title": "Search Tracks",
      "subtitle": "Spotify (BETA)",
      "description": "Search & Play tracks in Spotify",
      "mode": "view"
    },
    {
      "name": "searchArtists",
      "title": "Search Artists",
      "subtitle": "Spotify (BETA)",
      "description": "Search Artists in Spotify",
      "mode": "view"
    },
    {
      "name": "like_current_song",
      "title": "Like Currently Playing Song",
      "subtitle": "Spotify (BETA)",
      "description": "Likes currently playing song",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.33.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/spotify-web-api-node": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "js-base64": "^3.6.1",
    "open": "^8.2.1",
    "react-devtools": "^4.19.2",
    "spotify-web-api-node": "^5.0.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}